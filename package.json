{
  "name": "shri-2021-restorator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "delivery": "npm run push && npm-run-all delivery:*",
    "delivery:clone": "npx pm2 deploy production setup",
    "delivery:build": "npx pm2 deploy production exec \"npm i && npm run build\"",
    "delivery:start": "pm2 start npm --name \"next\" -- start",
    "push": "rsync -az --delete --delete-excluded --exclude-from '.gitignore' . restart@178.154.221.21:/home/restorator/src/repo",
    "deploy": "npm run push && npx pm2 deploy production exec \"git pull && npm i && npm run build && pm2 restart next\"",
    "prepare": "husky install",
    "lint": "npm run pretty-quick && npm run lint:next && npm run lint:css",
    "lint:next": "next lint",
    "lint:css": "npx stylelint '**/*.(s)?css'",
    "pretty-quick": "pretty-quick --staged"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "11.0.1",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-flowtype": "^5.8.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "4.3.5"
  }
}
