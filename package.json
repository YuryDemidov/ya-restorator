{
  "name": "shri-2021-restorator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "delivery": "npm run push && npm-run-all delivery:*",
    "delivery:clone": "npx pm2 deploy production setup",
    "delivery:build": "npx pm2 deploy production exec \"npm i && npm run build\"",
    "delivery:start": "pm2 start npm --name \"next\" -- start",
    "push": "rsync -az --delete --delete-excluded --exclude-from '.gitignore' . restart@178.154.221.21:/home/restorator/src/repo",
    "deploy": "npm run push && npx pm2 deploy production exec \"git pull && npm i && npm run build && pm2 restart next\""
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.15",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.0",
    "typescript": "4.3.5"
  }
}
